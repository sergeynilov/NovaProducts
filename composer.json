{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "antoniosiles/nova-4-card-map-plus": "^0.0.28",
        "creode/collapsible-radios": "^1.1",
        "ctessier/nova-advanced-image-field": "^2.1",
        "cviebrock/eloquent-sluggable": "^10.0",
        "cybercog/laravel-nova-ban": "^2.0",
        "devloops/phone-number": "^1.0",
        "digital-creative/column-toggler": "^0.1.1",
        "digital-creative/nova-welcome-card": "^0.0.2",
        "digital-creative/table-widget": "^1.0",
        "ebess/advanced-nova-media-library": "^4.2",
        "eminiarts/nova-tabs": "^2.2",
        "guzzlehttp/guzzle": "^7.2",
        "imumz/nova-4-field-map": "^1.0",
        "imumz/nova-accordion-field": "^1.0",
        "kirschbaum-development/nova-chartjs": "^2.0",
        "laravel/framework": "^10.10",
        "laravel/nova": "4.27.12",
        "laravel/nova-log-viewer": "^0.1.4",
        "laravel/sanctum": "^3.3",
        "laravel/telescope": "^5.2",
        "laravel/tinker": "^2.8",
        "maatwebsite/laravel-nova-excel": "^1.3",
        "markwalet/nova-modal-response": "^0.3.0",
        "marshmallow/nova-resource-click": "^1.0",
        "mateusz-peczkowski/nova-heartbeat-resource-field": "^1.0",
        "oneduo/nova-file-manager": "^0.12.4",
        "outl1ne/nova-page-manager": "^5.9",
        "outl1ne/nova-settings": "^5.2",
        "outl1ne/nova-simple-repeatable": "^2.2",
        "outl1ne/nova-sortable": "^3.4",
        "pavloniym/nova-action-buttons": "^1.1",
        "rpj/daterangepicker": "^0.0.8",
        "seche/nova-jstree": "^1.0",
        "shuvroroy/nova-tabs": "^1.1",
        "simonbarrettact/unsplash": "^0.0.6",
        "spatie/laravel-medialibrary": "^11.9",
        "spatie/laravel-permission": "^6.9",
        "timothyasp/nova-badge-field": "^2.0",
        "titasgailius/search-relations": "^2.0",
        "wdelfuego/nova-wizard": "^1.1"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "files": [
            "app/Library/helper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
